{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "WebWorker"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": [
      "@cloudflare/workers-types/2023-07-01"
    ],
    "noEmit": true
  },
  "include": ["src/**/*"]
}

